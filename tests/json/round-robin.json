{
  "vhosts": [
    {
      "server_name": "localhost",
      "ip": "0.0.0.0",
      "port": 8000,
      "proxy_pass": {
        "upstream": "rb"
      }
    }
  ],
  "upstreams": {
    "rb": {
      "method": "round-robin",
      "hosts": [
        {
          "ip": "127.0.0.1",
          "port": 80001,
          "weight": 3
        },
        {
          "ip": "127.0.0.1",
          "port": 8002,
          "weight": 1
        }
      ]
    }
  }
}